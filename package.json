{
  "name": "unicode-eingabe-kiwi3",
  "version": "2.0.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest run",
    "build": "vue-tsc --build --noCheck && vite build && npm run copy-locales",
    "copy-locales": "mkdir -p dist/locales && cp -r src/locales/* dist/locales/",
    "lint": "prettier src/ tests/ --check && eslint . && vue-tsc --noEmit",
    "format": "prettier src/ tests/ --write",
    "fix": "prettier . --write && eslint . --fix"
  },
  "dependencies": {
    "@fontsource/roboto": "5.2.9",
    "@types/webfontloader": "^1.6.38",
    "@vueuse/core": "13.5.0",
    "pinia": "3.0.3",
    "pinia-plugin-persistedstate": "^4.3.0",
    "vue": "3.5.28",
    "vue-i18n": "11.2.8",
    "vue-router": "5.0.3",
    "vuetify": "3.10.7",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@mdi/js": "7.4.47",
    "@muenchen/prettier-codeformat": "1.0.2",
    "@tsconfig/node-lts": "22.0.2",
    "@types/node": "24.2.1",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "eslint": "9.39.2",
    "eslint-plugin-vue": "10.5.1",
    "jsdom": "26.1.0",
    "prettier": "3.6.2",
    "typescript": "5.8.3",
    "unplugin-fonts": "1.3.1",
    "vite": "6.4.1",
    "vite-plugin-vue-devtools": "7.7.7",
    "vite-plugin-vuetify": "2.1.3",
    "vitest": "3.2.4",
    "vue-tsc": "3.1.2"
  }
}
