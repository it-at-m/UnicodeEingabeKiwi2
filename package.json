    "vue-router": "4.5.1",
    "vuetify": "3.8.9",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@mdi/js": "7.4.47",
    "@muenchen/prettier-codeformat": "1.0.2",
    "@tsconfig/node-lts": "22.0.1",
    "@types/node": "22.15.31",
    "@vitejs/plugin-vue": "5.2.4",
